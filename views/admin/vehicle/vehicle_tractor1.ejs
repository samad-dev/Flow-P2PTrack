                        keys: !0, language: {
                            paginate:
                                { previous: "<i class='ri-arrow-left-s-line'>", next: "<i class='ri-arrow-right-s-line'>" }
                        },
                        drawCallback:
                            function () { $(".dataTables_paginate > .pagination").addClass("pagination-rounded") }
                    });


                })

                function parkingloc(id) {
                    if (id == "ALL") {


                        var settings = {
                            "url": "parkingloc/",
                            "method": "GET",
                            "timeout": 0,
                        };
                    }
                    else {
                        var settings = {
                            "url": "parkingloc_byscopes/" + id,
                            "method": "GET",
                            "timeout": 0,
                        };
                    }

                    $.ajax(settings).done(function (response) {
                        customer_tbl.clear().draw();
                        $.each(response, function (index, data) {
                            

                        });
                    });
                }


                function tractor() {
                    $.ajax({
                        url: "vehicle_tracktors/<%- id%>",
                        type: 'GET',
                        dataType: 'json',
                        success: function (response) {

                            $('#tracktor_code').val(response[0]['tractor_code']);
                            $('#reg_no').val(response[0]['tractor_reg']);
                            $('#weight').val(response[0]['cvweight']);

                            $('#scopesel').val(response[0]['scope']).trigger('change');
                            $('#discharge_factor').val(1);
                            $('#exp_date').val(response[0]['expiry_date']);
                            setTimeout(() => {
                                $('#parking_locs').val(response[0]['parking_location']).trigger('change');

                            }, 1000)

                        }
                    });
                }
                $('#modal_save').click(() => {
                    update_id = $('#update_id').val();
                    if (update_id == "") {
                        var form = new FormData();
                        form.append("vehicle_id", '<%- id %>');
                        form.append("start_day", $('#strt_day').val());
                        form.append("start_time", $('#str_time').val());
                        form.append("end_day", $('#end_day').val());
                        form.append("end_time", $('#end_time').val());
                        form.append("availability", $('#availability').val());
                        form.append("shift", $('#shift').val());
                        form.append("start_location", $('#str_loc').val());
                        form.append("end_location", $('#end_loc').val());
